  "logo": "https://github.com/Prince527GitHub/ServerSMP-BOT/blob/web/assets/serversmp-bot.png?raw=true",
  "env": {
    "TOKEN": {
      "description": "Your bot's token",
      "required": true
    },
    "PREFIX": {
      "description": "Your bot's prefix",
      "required": true
    },
    "MONGO": {
      "description": "Your mongodb url",
      "required": true
